<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:tns="http://ais-gzhi.ru/schema/integration/eds/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" targetNamespace="http://ais-gzhi.ru/schema/integration/eds/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.0.1">
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd"/>
	<xs:element name="getNsiDSRequest">
		<xs:annotation>
			<xs:documentation>Запрос данных справочников организаций.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:BaseType">
					<xs:sequence>
						<xs:choice>
							<xs:element name="AppealStatus" type="xs:boolean" fixed="true">
								<xs:annotation>
									<xs:documentation>Статусы Заявок на обслуживание</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AppealCancelReasons" type="xs:boolean" fixed="true">
								<xs:annotation>
									<xs:documentation>Причины не выполнения</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AppealProlongReasons" type="xs:boolean" fixed="true">
								<xs:annotation>
									<xs:documentation>Причины переноса</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AppealType" type="xs:boolean" fixed="true">
								<xs:annotation>
									<xs:documentation>Тип заявки</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="AppealTypeWorks" type="xs:boolean" fixed="true">
								<xs:annotation>
									<xs:documentation>Классификатор типов работ</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Orgs" type="xs:boolean" fixed="true">
								<xs:annotation>
									<xs:documentation>Организации</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Accomodation" type="tns:GUIDType">
								<xs:annotation>
									<xs:documentation>Помещения по дому (Указывается AddressGUID дома, возвращается список помещений)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Addresses">
								<xs:annotation>
									<xs:documentation>Адреса обслуживаемых домов</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="OrgManager" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Организация, управляющая домом</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:sequence>
					<xs:attribute ref="tns:version" use="required" fixed="1.0.0.1"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="importAppealRequest">
		<xs:annotation>
			<xs:documentation>Импорт лицевых счетов.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:BaseType">
					<xs:sequence>
						<xs:element name="Appeal" maxOccurs="1000">
							<xs:annotation>
								<xs:documentation>Перечень Заявок на обслуживание</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="tns:AppealGUID"/>
									<xs:element ref="tns:TransportGUID"/>
									<xs:choice>
										<xs:element name="AppealInformation">
											<xs:annotation>
												<xs:documentation>Добавление и изменение</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="CreationDate" type="xs:dateTime">
														<xs:annotation>
															<xs:documentation>Дата и время создания заявки в ИС-Источник</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Status" type="tns:nsiRef">
														<xs:annotation>
															<xs:documentation>Статус заявки</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Initiator">
														<xs:annotation>
															<xs:documentation>Инициатор заявки</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:all>
																<xs:element name="FIO" type="tns:String255Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>ФИО (или название организации) заявителя</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Mail" type="tns:String50Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>E-mail заявителя</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Phone" type="tns:String50Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Телефон заявителя</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="TypeAppeal" type="tns:nsiRef">
														<xs:annotation>
															<xs:documentation>Тип заявки</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TypeWorkGUID" type="tns:GUIDType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Тип работ по заявке</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:choice>
														<xs:element name="Address" type="tns:String255NotEmptyType">
															<xs:annotation>
																<xs:documentation>Адрес дома</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="AddressGUID" type="tns:GUIDType">
															<xs:annotation>
																<xs:documentation>Идентификатор адреса дома</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:choice>
													<xs:element name="Text" type="tns:String1000NotEmptyType">
														<xs:annotation>
															<xs:documentation>Текст обращения в заявке</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="OrgGUID" type="tns:GUIDType">
														<xs:annotation>
															<xs:documentation>Организация – исполнитель заявки </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="NumberReg" type="tns:String255NotEmptyType">
														<xs:annotation>
															<xs:documentation>Регистрационный номер заявки в ИС-источнике</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DateReg" type="xs:date">
														<xs:annotation>
															<xs:documentation>Дата регистрации заявки в ИС-источнике</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DatePlan" type="xs:dateTime">
														<xs:annotation>
															<xs:documentation>Плановая дата исполнения заявки</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Prolong" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Переноса даты исполнения заявки</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:all>
																<xs:element name="DatePlan" type="xs:dateTime">
																	<xs:annotation>
																		<xs:documentation>Дата переноса исполнения заявки</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ProlongReasons" type="tns:nsiRef" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Причина переноса даты исполнения заявки</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ProlongComments" type="tns:String1000Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Комментарии к причине переноса даты исполнения заявки</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:all>														
														</xs:complexType>
													</xs:element>
													<xs:element name="Fact" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Факт исполнения заявки</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:all>
																<xs:element name="DateFact" type="xs:dateTime">
																	<xs:annotation>
																		<xs:documentation>Фактическая дата исполнения заявки</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="IsSignedAct" type="xs:boolean">
																	<xs:annotation>
																		<xs:documentation>Акт подписан?</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="IsDone" type="xs:boolean">
																	<xs:annotation>
																		<xs:documentation>Заявка выполнена?</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="CancelReasons" type="tns:nsiRef" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Причина невыполнения заявки</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="CancelComments" type="tns:String1000Type" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Комментарии к причине невыполнения заявки</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="Description" type="tns:String1000Type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Примечание к заявке</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="CancelAppeal">
											<xs:annotation>
												<xs:documentation>Аннулирование заявки на обслуживание</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Description" type="tns:String250Type" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Примечание</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute ref="tns:version" use="required" fixed="1.0.0.1"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="importAppeal_TransportGUIDKey">
			<xs:selector xpath=".//tns:TransportGUID"/>
			<xs:field xpath="."/>
		</xs:key>
	</xs:element>
	<xs:element name="getObjectStateDSRequest">
		<xs:annotation>
			<xs:documentation>Запрос о состоянии объектов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:BaseType">
					<xs:sequence>
						<xs:element name="APIName">
							<xs:annotation>
								<xs:documentation>Наименование API</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="ImportAppealData"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:choice>
							<xs:element name="ObjectGUID" type="tns:GUIDType" maxOccurs="1000">
								<xs:annotation>
									<xs:documentation>Идентификатор запрашиваемого объекта</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="RequestPackGUID" type="tns:GUIDType">
								<xs:annotation>
									<xs:documentation>Ранее переданный пакет</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:sequence>
					<xs:attribute ref="tns:version" use="required" fixed="1.0.0.1"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Success">
		<xs:annotation>
			<xs:documentation>Ответ об успешном приеме запроса</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PackGUID" type="tns:GUIDType">
					<xs:annotation>
						<xs:documentation>Уникальный идентификатор пакета обмена</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="getStateDSRequest">
		<xs:annotation>
			<xs:documentation>Запрос о состоянии пакета  </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:BaseType">
					<xs:sequence>
						<xs:element name="PackGUID" type="tns:GUIDType">
							<xs:annotation>
								<xs:documentation>Идентификатор запрашиваемого пакета</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute ref="tns:version" use="required" fixed="1.0.0.1"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="getStateDSResult">
		<xs:annotation>
			<xs:documentation>Ответ о состоянии пакета </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:BaseType">
					<xs:sequence>
						<xs:element name="APIName">
							<xs:annotation>
								<xs:documentation>Наименование API</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="ImportAppealData"/>
									<xs:enumeration value="GetNsiDS"/>
									<xs:enumeration value="GetObjectStateDS"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="PackState">
							<xs:annotation>
								<xs:documentation>Ответ на запрос о состоянии запрашиваемого пакета:
												10	Получено
												20	В обработке
												30	Обработан
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="tns:nsiRef">
									<xs:length value="2"/>
										<xs:enumeration value="10"/>
										<xs:enumeration value="20"/>
										<xs:enumeration value="30"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ERRORS" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Сообщения об ошибках, возникшие после обработки запрашиваемого пакета</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="tns:ErrorType">
										<xs:sequence>
											<xs:element name="ObjectGUID" type="tns:GUIDType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>GUID объекта в ИС-Источнике</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element ref="tns:TransportGUID" minOccurs="0"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:choice minOccurs="0">
							<xs:element name="GetNsiResult" type="tns:ReturnNsi">
								<xs:annotation>
									<xs:documentation>Информация о справочниках организации</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="GetObjectStateResult" type="tns:ReturnGetObjectState" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Информация о статусах объектов пакета</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:sequence>
					<xs:attribute ref="tns:version" use="required" fixed="1.0.0.1"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ReturnNsi">
		<xs:annotation>
			<xs:documentation>Информация о справочниках организации</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="AppealStatus" type="tns:NsiCodeWithName" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Статусы Заявок на обслуживание</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AppealCancelReasons" type="tns:NsiCodeWithName" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Причины не выполнения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AppealProlongReasons" type="tns:NsiCodeWithName" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Причины переноса</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AppealType" type="tns:NsiCodeWithName" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Типы заявок на обслуживание</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AppealTypeWorks" type="tns:AppealTypeWorksType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Классификатор типа работ</xs:documentation>
					</xs:annotation>
				</xs:element>
				
				<xs:element name="Orgs" type="tns:OrgSCType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Реестр организаций</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Addresses" type="tns:AddressElementType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Адреса обслуживаемых домов</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Accomodation">
					<xs:annotation>
						<xs:documentation>Помещения по дому</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AddressGUID" type="tns:GUIDType">
								<xs:annotation>
									<xs:documentation>Глобальный уникальный идентификатор адресного объекта в АИС ГЖИ</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Accomodations" type="tns:AccomodationType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Список помещений по дому</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReturnGetObjectState">
	    <xs:all>
	        <xs:element name="TransportInformation" minOccurs="0">
	            <xs:annotation>
	                <xs:documentation>Информация о транспорте</xs:documentation>
	            </xs:annotation>
	            <xs:complexType>
	                <xs:sequence>
	                    <xs:element ref="tns:TransportGUID"/>
	                    <xs:element name="TransportStatus" type="tns:nsiRef">
	                        <xs:annotation>
	                            <xs:documentation>Статус транспорта в АИС ГЖИ</xs:documentation>
	                        </xs:annotation>
	                    </xs:element>
	                    <xs:element name="TransportStatusDate" type="xs:dateTime">
	                        <xs:annotation>
	                            <xs:documentation>Дата и время установки статуса транспорта</xs:documentation>
	                        </xs:annotation>
	                    </xs:element>
	                    <xs:element ref="tns:ERRORS" minOccurs="0" maxOccurs="unbounded"/>
	                </xs:sequence>
	            </xs:complexType>
	        </xs:element>
	        <xs:element name="ObjectInformation" minOccurs="0">
	            <xs:annotation>
	                <xs:documentation>Информация об объекте</xs:documentation>
	            </xs:annotation>
	            <xs:complexType>
	                <xs:sequence>
	                    <xs:element name="ObjectGUID" type="tns:GUIDType">
	                        <xs:annotation>
	                            <xs:documentation>Идентификатор запрашиваемого объекта</xs:documentation>
	                        </xs:annotation>
	                    </xs:element>
	                    <xs:element name="ObjectState" type="tns:ObjectStateType">
	                        <xs:annotation>
	                            <xs:documentation>Статус запрашиваемого объекта</xs:documentation>
	                        </xs:annotation>
	                    </xs:element>
	                    <xs:element ref="tns:ERRORS" minOccurs="0" maxOccurs="unbounded"/>
	                </xs:sequence>
	            </xs:complexType>
	        </xs:element>
	    </xs:all>
	</xs:complexType>
	<xs:complexType name="ObjectStateType">
		<xs:all>
			<xs:element name="UpdateDate" type="xs:dateTime">
	        	<xs:annotation>
	            	<xs:documentation>Дата модификации описания объекта</xs:documentation>
	            </xs:annotation>
	       	</xs:element>
	       	<xs:element name="ObjectProcessingStatus" type="tns:nsiRef">
	        	<xs:annotation>
	           		<xs:documentation>Статус обработки объекта</xs:documentation>
	      		</xs:annotation>
	    	</xs:element>
	      	<xs:element name="ObjectProcessingStatusDate" type="xs:dateTime">
	          	<xs:annotation>
	             	<xs:documentation>Дата и время установки статуса обработки объекта</xs:documentation>
	         	</xs:annotation>
	   		</xs:element>
	   		<xs:element name="ObjectStatus" type="tns:nsiRef" minOccurs="0">
